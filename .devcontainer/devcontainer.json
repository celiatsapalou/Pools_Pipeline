{
    "name": "Python with Snakemake",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "settings": {
      "terminal.integrated.shell.linux": "zsh"
    },
    "postCreateCommand": "mamba activate snakemake-env",
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter",
      "tfehlmann.snakefmt",
      "snakemake.snakemake-lang",
      "GitHub.copilot"
    ]
  }
  